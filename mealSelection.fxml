        <TextField fx:id="priceTextField" editable="false" layoutX="369.0" layoutY="159.0" opacity="0.0" prefHeight="26.0" prefWidth="56.0" />
        <TextField fx:id="caloriesTextField" editable="false" layoutX="369.0" layoutY="198.0" opacity="0.0" prefHeight="26.0" prefWidth="56.0" />
        <Button fx:id="moreInfoButton" layoutX="168.0" layoutY="242.0" mnemonicParsing="false" onAction="#showMoreInfo" text="More info" />
