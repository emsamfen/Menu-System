<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="143.0" prefWidth="287.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Example.OwnerOptionsController">
    <children>
        <Button fx:id="setNoOfTablesButton" disable="true" layoutX="13.0" layoutY="56.0" mnemonicParsing="false" onAction="#setNoOfTables" opacity="0.0" text="Set number of tables" />
        <Button fx:id="editMenuButton" disable="true" layoutX="197.0" layoutY="56.0" mnemonicParsing="false" onAction="#editMenu" opacity="0.0" text="Edit Menu" />
        <Button fx:id="doneButton" layoutX="222.0" layoutY="111.0" mnemonicParsing="false" onAction="#done" text="Done" />
        <Label fx:id="passwordLabel" layoutX="76.0" layoutY="14.0" text="Please enter password:" />
        <TextField fx:id="passwordTF" layoutX="62.0" layoutY="44.0" />
        <Button fx:id="enterButton" layoutX="114.0" layoutY="99.0" mnemonicParsing="false" onAction="#enterPassword" text="Enter" />
      <Label fx:id="attemptsLeftTF" layoutX="66.0" layoutY="72.0" opacity="0.0" text="Incorrect - Attempts left: X" />
    </children>
</AnchorPane>
